.wrapper.wrapper-style4
  article#contact.container.small
    header
      h2 Want to hire me? Get in touch!
      span Ornare nulla proin odio consequat sapien vestibulum ipsum sed lorem.
    div
      .row
        .12u
          form(method='post', action='#')
            div
              .row.half
                .6u
                  input#name(type='text', name='name', placeholder='Name')
                .6u
                  input#email(type='text', name='email', placeholder='Email')
              .row.half
                .12u
                  input#subject(type='text', name='subject', placeholder='Subject')
              .row.half
                .12u
                  textarea#message(name='message', placeholder='Message')
              .row
                .12u
                  a.button.form-button-submit(href='#') Send Message
                  a.button.button-alt.form-button-reset(href='#') Clear Form
      .row
        .12u
          hr
          h3 Find me on ...
          ul.social
            li.twitter
              a.fa.fa-twitter(href='http://twitter.com/n33co')
                span Twitter
            li.facebook
              a.fa.fa-facebook(href='#')
                span Facebook
            li.dribbble
              a.fa.fa-dribbble(href='http://dribbble.com/n33')
                span Dribbble
            li.linkedin
              a.fa.fa-linkedin(href='#')
                span LinkedIn
            li.tumblr
              a.fa.fa-tumblr(href='#')
                span Tumblr
            li.googleplus
              a.fa.fa-google-plus(href='#')
                span Google+
            li.github
              a.fa.fa-github(href='http://github.com/n33')
                span Github
